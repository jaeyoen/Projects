    "    # classifying the predicted labels by comparing the front 47999 data to 48000~59999.\n",
    "    predicted_label[t] = knn_classifier(trained_data,trained_label,images[54000+t,:], 10)[0][0][0]\n",
    "    # real label \n",
    "    real_label[t] = labels[54000+t]\n",
    "\n",
    "predicted_label,real_label"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "2b2d887e",
   "metadata": {},
   "outputs": [],
   "source": [
    "y_true= real_label # Array of real labels\n",
    "y_pred= predicted_label # Array of predicted labels\n",
    "c =confusion_matrix(y_true, y_pred) # Confusion matrix"
   ]
  },
  {
   "cell_type": "code",
